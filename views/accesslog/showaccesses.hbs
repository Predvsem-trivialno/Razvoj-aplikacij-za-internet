 <div class="container h-100">
    <div class="row d-flex justify-content-center align-items-center h-200">
      <div class="col-lg-12 col-xl-12">
        <div class="card text-black" style="border-radius: 25px;">
          <div class="card-body p-md-5">
            <div class="row justify-content-center">
                <p class="text-center h2 fw-bold mb-5 mx-1 mx-md-10 mt-4">Access log for Box {{id}}</p>
                <table class="table table-hover">
                    <thead>
                    <tr>
                        <th>Opener</th>
                        <th>Date</th>
                        <th>Success</th>
                    </tr>
                    </thead>
                    <tbody>
                    {{#each accesslogs}}
                        <tr>
                            <td>{{openedBy}}</td>
                            <td>{{dateOpened}}</td>
                            <td>{{success}}</td>
                            </tr>
                    {{/each}}
                    </tbody>
                </table>
                <div class="m-3">
                    <a href="/accesslog/{{id}}" class="btn btn-success mr-10"><i class="fas fa-solid fa-rotate-left"></i></a>
                </div>
                <div class="m-3">
                    <a href="/postbox/show" class="btn btn-secondary padding-15"><i class="fas fa-solid fa-person-walking-arrow-loop-left"></i></a>
                </div>
                
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>